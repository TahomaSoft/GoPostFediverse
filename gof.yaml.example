accounts:
- name: instance.nice
  accesstoken: 
  instanceurl: https://yourinstance.nice
  feeds:
  - url: https://yourfeed.com
    template: '{{.Title}} {{.URL}}'
    
    # Format
    # One of:
    #   - plain
    #   - bbcode (pleroma only)
    #   - html (pleroma only)
    #   - markdown (pleroma only)
    # Default: plain
    format: markdown

    # Visibility
    # One of:
    #   - public
    #   - unlisted
    #   - private
    # Default: unlisted
    visibility: unlisted

    # Sensitivity
    #   true or false
    # Default: false
    sensitive: false
- name: freeradical.zone
  accesstoken:
  instanceurl: https://freeradical.zone
  feeds:
  - url: https://your.otherfeed.net
    template: |-
      {{.Title}}

      {{.URL}}
